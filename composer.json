{
    "name": "uteq/laravel-move-permissions",
    "description": "Permissions for Laravel Move Admin Panel",
    "keywords": [
        "uteq",
        "laravel-move-permissions"
    ],
    "homepage": "https://github.com/uteq/laravel-move-permissions",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Jansen",
            "email": "info@nathanjansen.nl",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-permission": "^3.17"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Uteq\\Permissions\\": "src",
            "Uteq\\Permissions\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uteq\\Permissions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uteq\\Permissions\\PermissionsServiceProvider"
            ],
            "aliases": {
                "Permissions": "Uteq\\Permissions\\PermissionsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
